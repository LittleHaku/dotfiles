{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.36/schema.bar.json",
  "monitor": 0,
  "font_family": "JetBrains Mono",
  "theme": {
    "colours": {
      "base_00": "#282936",
      "base_01": "#3a3c4a",
      "base_02": "#4d4f68",
      "base_03": "#626483",
      "base_04": "#62d6e8",
      "base_05": "#e9e9f4",
      "base_06": "#f1f2f8",
      "base_07": "#f7f7fb",
      "base_08": "#ea51b2",
      "base_09": "#b45bcf",
      "base_0a": "#00f769",
      "base_0b": "#ebff87",
      "base_0c": "#8be9fd",
      "base_0d": "#62d6e8",
      "base_0e": "#b45bcf",
      "base_0f": "#00f769"
    },
    "palette": "Custom",
    "accent": "Base07",
    "auto_select_fill": "Base09",
    "auto_select_text": "Base03"
  },
  "height": 25,
  "icon_scale": 1.25,
  "transparency_alpha": 75,
  "left_widgets": [
    {
      "Komorebi": {
        "workspaces": {
          "enable": true,
          "hide_empty_workspaces": false,
          "display": "AllIconsAndText"
        },
        "layout": {
          "enable": true,
          "display": "IconAndTextOnSelected"
        },
        "focused_window": {
          "enable": false,
          "show_icon": true
        }
      }
    }
  ],
  "right_widgets": [
    {
      "Update": {
        "enable": false
      }
    },
    {
      "Media": {
        "enable": true
      }
    },
    {
      "Storage": {
        "enable": false
      }
    },
    {
      "Memory": {
        "enable": true,
        "label_prefix": "Icon"
      }
    },
    {
      "Cpu": {
        "enable": true,
        "label_prefix": "Icon"
      }
    },
    {
      "Network": {
        "enable": false,
        "show_total_data_transmitted": false,
        "show_network_activity": false
      }
    },
    {
      "Date": {
        "enable": true,
        "format": "DateMonthYear"
      }
    },
    {
      "Time": {
        "enable": true,
        "format": "TwentyFourHour"
      }
    },
    {
      "Battery": {
        "enable": false
      }
    }
  ]
}
